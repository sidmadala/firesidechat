A;;AAAA;AAAA,uBAMA,IAAA,CAAA,8BAAA,CAAA,KAAA,CAAA,KAAA,EAAA,KAAA,CAAA,QAAA,CANA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA,uBAQA,IAAA,CAAA,0BAAA,CAAA,KAAA,CAAA,KAAA,EAAA,KAAA,CAAA,QAAA,CARA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAWA,gBAAA,KAAA,CAAA,YAAA,GAAA,YAAA,OAAA;;AAXA;AAcA,gBAAA,KAAA,CAAA,OAAA,GAAA,KAAA;;AAdA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAeA;AAjBA;AAZA,CAAA","sourcesContent":["<template>\n  <aside class=\"section\">\n    \n    <div v-if=\"newUser\">\n      <h3>Sign Up for a New Account</h3>\n      <a href=\"#\" @click=\"newUser = false\">Returning User?</a>\n    </div>\n\n    <div v-else>\n      <h3>Sign In with Email</h3>\n      <a href=\"#\" @click=\"newUser = true\">New user?</a>\n    </div>\n\n    <label for=\"email\">Email</label>\n    <br />\n    <input v-model=\"email\" placeholder=\"email\" type=\"email\" class=\"input\" />\n\n    <label for=\"password\">Password</label>\n    <br />\n    <input v-model=\"password\" type=\"password\" class=\"input\" />\n\n    <br />\n\n    <button\n      class=\"button is-info\"\n      :class=\"{ 'is-loading': loading }\"\n      @click=\"signInOrCreateUser()\"\n    >{{ newUser ? 'Sign Up' : 'Login'}}</button>\n\n    <p class=\"has-text-danger\" v-if=\"errorMessage\">{{ errorMessage }}</p>\n  </aside>\n</template>\n\n<script>\nimport { auth } from '../firebase';\n\n\nexport default {\n  data() {\n    return {\n      auth,\n      newUser: false,\n      email: '',\n      password: '',\n      errorMessage: '',\n      loading: false\n    }\n  },\n\n  methods: {\n      // asynchronous read/write from/to firebase for user information or creating an account\n      async signInOrCreateUser() {\n \n        this.loading = true;\n        this.errorMessage = '';\n        try {\n          if (this.newUser) {\n            await auth.createUserWithEmailAndPassword(this.email, this.password)\n          } else {\n            await auth.signInWithEmailAndPassword(this.email, this.password)\n          }\n        } catch (error) {\n            this.errorMessage = error.message;\n        }\n\n        this.loading = false;\n      }\n\n  },\n}\n\n</script>\n\n"],"sourceRoot":"src/components"}]}