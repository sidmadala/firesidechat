restore() {\n    return { \n        chats: db.collection('chats').where('owner', '==', this.uid) \n    }\n  },\n  methods: {\n      async createChatRoom() {\n          const newChat = await db.collection('chats').add({\n              createdAt: Date.now(),\n              owner: this.uid,\n              members: [this.uid]\n          })\n\n            console.log(newChat)\n      }\n\n\n      \n  },\n  props: ['uid']\n\n};\n</script>"]}]}